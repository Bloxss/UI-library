-- if the ui is messed up go to Display Settings and change scale to 100%
local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Bloxss/Scripts/main/Cool%20Ui%20Library')))()
local window = library.UI("Cool UI")

local tab = window:Tab("Tab",8930053558)

local container1 = tab:Container("Container1")

local container2 = tab:Container("Container2")

container1:Button("Test", function()
    print("Test")
end)

Container1:Toggle("Toggle", false, function(v)
    print(v)
end)
